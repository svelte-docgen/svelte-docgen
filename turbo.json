{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build:pkg": {
			"cache": true,
			"inputs": ["$TURBO_DEFAULT$", "src/**"],
			"outputs": ["./types"]
		},
		"//#build:doc": {
			"cache": true,
			"inputs": ["$TURBO_DEFAULT$", "src/**"],
			"outputs": ["./docs"]
		},
		"check": {
			"cache": true
		},
		"//#_test": {
			"cache": true,
			"inputs": ["src/**", "test/**"],
			"outputs": [".coverage/**"]
		}
	},
	"remoteCache": {
		"signature": true
	}
}
