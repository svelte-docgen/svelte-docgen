{
	"$schema": "https://json.schemastore.org/package",
	"name": "@svelte-docgen/server",
	"version": "0.0.0",
	"type": "module",
	"description": "Allows creating virtual modules with docgen data for targetted Svelte component(s) - on demand.",
	"keywords": [
		"svelte",
		"documentation",
		"component",
		"props",
		"docgen",
		"server"
	],
	"license": "MIT",
	"contributors": [
		{
			"name": "Mateusz Kadlubowski",
			"email": "xeho91@pm.me",
			"url": "https://github.com/xeho91"
		},
		{
			"name": "Taku Fukada",
			"email": "naninunenor@gmail.com",
			"url": "https://github.com/ciscorn"
		}
	],
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/svelte-docgen"
		},
		{
			"type": "opencollective",
			"url": "https://opencollective.com/svelte-docgen"
		}
	],
	"homepage": "https://github.com/svelte-docgen/svelte-docgen",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svelte-docgen/svelte-docgen.git",
		"directory": "packages/vite-plugin-svelte-docgen"
	},
	"bugs": "https://github.com/svelte-docgen/svelte-docgen/issues",
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"src/",
		"!src/**/*.test.*",
		"types/"
	],
	"exports": {
		".": {
			"types": "./types/mod.d.ts",
			"default": "./src/mod.js"
		},
		"./package.json": "./package.json"
	},
	"types": "./types/mod.d.ts",
	"scripts": {
		"build": "pnpm run \"/^build:.*/\" ",
		"build:doc": "typedoc",
		"clean": "pnpm run \"/^clean:.*/\"; rm -rf \"./**/node_modules\" ",
		"clean:cache": "rm -rf \"./node_modules/.cache\" ",
		"clean:doc": "rm -rf \"./docs\" ",
		"clean:test": "rm -rf \"./coverage\" ",
		"clean:types": "rm -rf \"./types\" ",
		"dev": "pnpm run \"/^dev:.*/\" ",
		"dev:doc": "typedoc --watch",
		"dev:test": "pnpm vitest watch --passWithNoTests --ui --open=false --workspace \"../../vitest.workspace.ts\" --project \"@svelte-docgen/server\" ",
		"fix": "pnpm run \"/^fix:.*/\" ",
		"fix:biome": "biome check . --verbose --write",
		"fix:format": "biome format . --verbose --write",
		"fix:js": "biome lint . --verbose --fix --unsafe",
		"fix:typos": "typos --verbose --write-changes",
		"lint": "pnpm run \"/^lint:.*/\" ",
		"lint:biome": "biome check . --verbose",
		"lint:format": "biome format . --verbose",
		"lint:js": "biome lint . --verbose",
		"lint:ts": "tsc --noEmit",
		"lint:typos": "typos --verbose",
		"serve:doc": "serve \"./docs\" ",
		"test": "vitest run --passWithNoTests --workspace \"../../vitest.workspace.ts\" --project \"@svelte-docgen/server\" "
	},
	"dependencies": {
		"@hono/node-server": "^1.13.7",
		"@hono/valibot-validator": "^0.5.1",
		"devalue": "^5.1.1",
		"hono": "^4.6.13",
		"valibot": "1.0.0-beta.9"
	},
	"peerDependencies": {
		"svelte-docgen": "workspace:*"
	}
}
